{
    "name": "friendsofhyperf/websocket-cluster-addon",
    "description": "Websocket cluster addon base redis subscribe.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "require": {
        "hyperf/websocket-server": "3.0.*",
        "hyperf/utils": "3.0.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "hyperf/command": "3.0.*",
        "hyperf/di": "3.0.*",
        "hyperf/event": "3.0.*",
        "hyperf/framework": "3.0.*",
        "hyperf/process": "3.0.*",
        "hyperf/redis": "3.0.*",
        "hyperf/server": "3.0.*",
        "hyperf/signal": "3.0.*",
        "hyperf/testing": "3.0.*",
        "mix/redis-subscriber": "^3.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0|^10.0",
        "swoole/ide-helper": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\WebsocketClusterAddon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FriendsOfHyperf\\WebsocketClusterAddon\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\WebsocketClusterAddon\\ConfigProvider"
        }
    }
}